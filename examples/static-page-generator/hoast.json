{
  "options": {
    "remove": true
  },
  "modules": {
    "hoast-filter": {
      "patterns": [
        "*",
        "!(layouts/*)"
      ],
      "patternOptions": {
        "all": true,
        "extended": true
      }
    },
    "read": {},
    "hoast-frontmatter": {},
    "hoast-transform": {
      "patterns": "*.md"
    },
    "hoast-layout": {
      "directory": "layouts",
      "layout": "page.hbs",
      "patterns": "*.html"
    }
  }
}