{
  "options": {
    "remove": true,
    "patterns": [
      "*",
      "!(layouts/*)"
    ],
    "patternOptions": {
      "all": true,
      "extended": true
    }
  },
  "modules": {
    "read": {},
    "hoast-frontmatter": {},
    "hoast-transform": {
      "patterns": "*.md"
    },
    "hoast-layout": {
      "directory": "layouts",
      "layout": "page.hbs",
      "patterns": "*.html"
    }
  }
}