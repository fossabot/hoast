{
  "options": {
    "remove": true
  },
  "modules": {
    "hoast-filter": {
      "patterns": "layouts/**",
      "patternOptions": {
        "all": true,
        "extended": true,
        "globstar": true
      }
    },
    "read": {},
    "hoast-frontmatter": {},
    "hoast-transform": {
      "patterns": "**/*.md",
      "patternOptions": {
        "globstar": true
      }
    },
    "hoast-layout": {
      "directory": "layouts",
      "layout": "page.hbs",
      "patterns": "**/*.html",
      "patternOptions": {
        "globstar": true
      }
    }
  }
}